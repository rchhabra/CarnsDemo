<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/util
           http://www.springframework.org/schema/util/spring-util.xsd">

  <import resource="travelrulesengine-compiler-spring.xml"/>

  <!-- Bean definitions for drools -->

  <alias name="defaultTravelRuleEngineCalculationService" alias="travelRuleEngineCalculationService"/>
  <bean id="defaultTravelRuleEngineCalculationService" parent="defaultRuleEngineCalculationService"
        class="de.hybris.platform.travelrulesengine.calculation.impl.DefaultTravelRuleEngineCalculationService">
  </bean>

  <alias name="defaultTravelRuleActionService" alias="travelRuleActionService"/>
  <bean id="defaultTravelRuleActionService" parent="defaultRuleActionService">
    <property name="actionStrategiesMapping" ref="travelActionStrategiesMapping"/>
  </bean>

  <alias name="defaultTravelFactContextFactory" alias="travelFactContextFactory"/>
  <bean id="defaultTravelFactContextFactory" parent="defaultFactContextFactory">
    <property name="raoProviders" ref="travelConfigRaoProviders" />
  </bean>

   <util:map id="travelConfigRaoProviders">
	<entry key="CART" value-ref="travelCartRaoProviders" />
	<entry key="FEE" value-ref="feeRaoProviders" />
	<entry key="FILTER_FARE" value-ref="filterFareRaoProviders" />
	<entry key="REFUND" value-ref="refundRaoProviders" />
	<entry key="FILTER_BUNDLE" value-ref="filterBundleRaoProviders" />
	<entry key="SHOW_BUNDLE_TEMPLATE" value-ref="showBundleTemplateRaoProviders" />
	<entry key="FILTER_TRANSPORTOFFERING" value-ref="filterTransportOfferingRaoProviders" />
    <entry key="SHOW_PRODUCTS" value-ref="showProductsRaoProviders" />
    <entry key="SHOW_CATEGORIES" value-ref="showProductsRaoProviders" />
  </util:map>

  <util:map id="travelCartRaoProviders">
    <entry key="de.hybris.platform.core.model.order.CartModel">
      <util:list>
        <ref bean="cartRaoProvider"/>
      </util:list>
    </entry>
    <entry key="de.hybris.platform.core.model.order.OrderModel">
      <util:list>
        <ref bean="bookingRaoProvider"/>
      </util:list>
    </entry>
  </util:map>

  <util:map id="feeRaoProviders">
    <entry key="de.hybris.platform.core.model.order.CartModel">
      <util:list>
        <ref bean="cartRaoProvider"/>
      </util:list>
    </entry>
    <entry key="de.hybris.platform.core.model.order.OrderModel">
      <util:list>
        <ref bean="bookingRaoProvider"/>
      </util:list>
    </entry>
  </util:map>

  <util:map id="refundRaoProviders">
    <entry key="de.hybris.platform.core.model.order.OrderModel">
      <util:list>
        <ref bean="bookingRaoProvider"/>
      </util:list>
    </entry>
  </util:map>

  <util:map id="filterFareRaoProviders">
    <entry key="de.hybris.platform.commercefacades.travel.FareProductData">
      <util:list>
        <ref bean="fareProductRaoProvider"/>
      </util:list>
    </entry>
    <entry key="de.hybris.platform.commercefacades.travel.FareSearchRequestData">
      <util:list>
        <ref bean="fareSearchRequestRaoProvider"/>
      </util:list>
    </entry>
  </util:map>

  <util:map id="filterBundleRaoProviders">
    <entry key="de.hybris.platform.core.model.user.UserModel">
      <util:list>
        <ref bean="userRaoProvider"/>
      </util:list>
    </entry>
    <entry key="de.hybris.platform.commercefacades.travel.FareSearchRequestData">
      <util:list>
        <ref bean="fareSearchRequestRaoProvider"/>
      </util:list>
    </entry>
  </util:map>

  <util:map id="showBundleTemplateRaoProviders">
    <entry key="de.hybris.platform.commercefacades.travel.FareSearchRequestData">
      <util:list>
        <ref bean="searchParameterRaoProvider"/>
      </util:list>
    </entry>
  </util:map>
  
  <util:map id="showProductsRaoProviders">
    <entry key="de.hybris.platform.commercefacades.travel.ancillary.data.OfferRequestData">
      <util:list>
        <ref bean="offerRequestRaoProvider"/>
      </util:list>
    </entry>
  </util:map>
  
  <util:map id="filterTransportOfferingRaoProviders">
    <entry key="de.hybris.platform.commercefacades.travel.TransportOfferingData">
      <util:list>
        <ref bean="transportOfferingRaoProvider"/>
      </util:list>
    </entry>
    <entry key="de.hybris.platform.commercefacades.travel.FareSearchRequestData">
      <util:list>
        <ref bean="fareSearchRequestRaoProvider"/>
      </util:list>
    </entry>
  </util:map>

  <bean id="abstractTravelEngineService" class="de.hybris.platform.travelrulesengine.services.impl.AbstractTravelEngineService"
        abstract="true">
    <property name="commerceRuleEngineService" ref="commerceRuleEngineService"/>
    <property name="ruleActionService" ref="travelRuleActionService"/>
    <property name="ruleEngineContextDao" ref="ruleEngineContextDao"/>
    <property name="factContextFactory" ref="travelFactContextFactory"/>
  </bean>

  <alias name="defaultTravelDroolsService" alias="travelDroolsService"/>
  <bean id="defaultTravelDroolsService" class="de.hybris.platform.travelrulesengine.services.impl.DefaultTravelDroolsService"
        parent="abstractTravelEngineService">
    <property name="defaultRuleEngineContextName" value="travel-rules-context"/>
  </bean>

  <!-- RAO Providers -->

  <alias name="defaultBookingRAOProvider" alias="bookingRaoProvider"/>
  <bean id="defaultBookingRAOProvider" class="de.hybris.platform.travelrulesengine.rao.providers.impl.DefaultBookingRAOProvider">
    <property name="bookingRaoConverter" ref="bookingRaoConverter"/>
    <property name="defaultOptions">
      <list>
        <value>INCLUDE_CANCEL_BOOKING</value>
        <value>EXPAND_BOOKING</value>
      </list>
    </property>
    <property name="timeService" ref="timeService"/>
  </bean>

  <alias name="defaultFareProductRAOProvider" alias="fareProductRaoProvider"/>
  <bean id="defaultFareProductRAOProvider"
        class="de.hybris.platform.travelrulesengine.rao.providers.impl.DefaultFareProductRAOProvider">
    <property name="fareProductRaoConverter" ref="fareProductRaoConverter"/>
    <property name="defaultOptions">
      <list>
        <value>INCLUDE_PRODUCT</value>
        <value>EXPAND_CATEGORIES</value>
      </list>
    </property>
  </bean>

  <alias name="defaultFareSearchRequestRaoProvider" alias="fareSearchRequestRaoProvider"/>
  <bean id="defaultFareSearchRequestRaoProvider"
        class="de.hybris.platform.travelrulesengine.rao.providers.impl.DefaultFareSearchRequestRaoProvider">
    <property name="fareSearchRequestRaoConverter" ref="fareSearchRequestRaoConverter"/>
    <property name="defaultOptions">
      <list>
        <value>INCLUDE_FARE_SEARCH</value>
        <value>EXPAND_LEG_INFOS</value>
        <value>EXPAND_PASSENGER_TYPE_QUANTITIES</value>
      </list>
    </property>
  </bean>

  <alias name="defaultSearchParameterRaoProvider" alias="searchParameterRaoProvider"/>
  <bean id="defaultSearchParameterRaoProvider"
        class="de.hybris.platform.travelrulesengine.rao.providers.impl.DefaultSearchParameterRaoProvider">
    <property name="searchParamsRaoConverter" ref="searchParamsRaoConverter"/>
    <property name="defaultOptions">
      <list>
        <value>INCLUDE_SEARCH_PARAMS</value>
        <value>EXPAND_LEG_INFOS</value>
        <value>EXPAND_USER</value>
        <value>EXPAND_USER_GROUP</value>
        <value>EXPAND_PASSENGER_TYPE_QUANTITIES</value>
      </list>
    </property>
  </bean>
  
  <alias name="defaultOfferRequestRaoProvider" alias="offerRequestRaoProvider"/>
  <bean id="defaultOfferRequestRaoProvider"
        class="de.hybris.platform.travelrulesengine.rao.providers.impl.DefaultOfferRequestRaoProvider">
    <property name="offerRequestRaoConverter" ref="offerRequestRaoConverter"/>
    <property name="defaultOptions">
      <list>
        <value>INCLUDE_OFFER_REQUEST</value>
        <value>EXPAND_LEG_INFOS</value>
        <value>EXPAND_USER</value>
        <value>EXPAND_USER_GROUP</value>
        <value>EXPAND_PASSENGER_TYPE_QUANTITIES</value>
      </list>
    </property>
  </bean>

  <alias name="defaultUserRaoProvider" alias="userRaoProvider"/>
  <bean id="defaultUserRaoProvider" class="de.hybris.platform.travelrulesengine.rao.providers.impl.DefaultUserRaoProvider">
    <property name="userRaoConverter" ref="userRaoConverter"/>
    <property name="defaultOptions">
      <list>
        <value>INCLUDE_USER</value>
        <value>EXPAND_GROUPS</value>
      </list>
    </property>
  </bean>

  <alias name="defaultTransportOfferingRAOProvider" alias="transportOfferingRaoProvider"/>
  <bean id="defaultTransportOfferingRAOProvider"
        class="de.hybris.platform.travelrulesengine.rao.providers.impl.DefaultTransportOfferingRAOProvider">
    <property name="transportOfferingRaoConverter" ref="transportOfferingRaoConverter"/>
  </bean>

  <alias name="defaultTravelCartRAOProvider" alias="cartRaoProvider" />
  <bean id="defaultTravelCartRAOProvider" class="de.hybris.platform.travelrulesengine.rao.providers.impl.DefaultTravelCartRAOProvider" parent="defaultCartRAOProvider">
    <property name="travelOptions">
      <list>
        <value>EXPAND_PASSENGER_TYPE_QUANTITIES</value>
      </list>
    </property>
  </bean>

  <!-- RAO Converters Populators -->

  <alias name="defaultBookingRaoConverter" alias="bookingRaoConverter"/>
  <bean id="defaultBookingRaoConverter" parent="abstractPopulatingConverter">
    <property name="targetClass" value="de.hybris.platform.travelrulesengine.rao.BookingRAO"/>
    <property name="populators">
      <list>
        <ref bean="bookingRaoPopulator"/>
      </list>
    </property>
  </bean>
  <alias name="defaultBookingRaoPopulator" alias="bookingRaoPopulator"/>
  <bean id="defaultBookingRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.BookingRaoPopulator"/>

  <alias name="defaultFareProductRaoConverter" alias="fareProductRaoConverter"/>
  <bean id="defaultFareProductRaoConverter" parent="abstractPopulatingConverter">
    <property name="targetClass" value="de.hybris.platform.travelrulesengine.rao.FareProductRAO"/>
    <property name="populators">
      <list>
        <ref bean="fareProductRaoPopulator"/>
      </list>
    </property>
  </bean>
  <alias name="defaultFareProductRaoPopulator" alias="fareProductRaoPopulator"/>
  <bean id="defaultFareProductRaoPopulator"
        class="de.hybris.platform.travelrulesengine.converters.populator.FareProductRaoPopulator">
    <property name="categoryDataRaoConverter" ref="categoryDataRaoConverter"/>
  </bean>

  <alias name="defaultTransportOfferingRaoConverter" alias="transportOfferingRaoConverter"/>
  <bean id="defaultTransportOfferingRaoConverter" parent="abstractPopulatingConverter">
    <property name="targetClass" value="de.hybris.platform.travelrulesengine.rao.TransportOfferingRAO"/>
    <property name="populators">
      <list>
        <ref bean="transportOfferingRaoPopulator"/>
      </list>
    </property>
  </bean>
  <alias name="defaultTransportOfferingRaoPopulator" alias="transportOfferingRaoPopulator"/>
  <bean id="defaultTransportOfferingRaoPopulator"
        class="de.hybris.platform.travelrulesengine.converters.populator.TransportOfferingRaoPopulator">
  </bean>

  <alias name="defaultCategoryDataRaoConverter" alias="categoryDataRaoConverter"/>
  <bean id="defaultCategoryDataRaoConverter" parent="abstractPopulatingConverter">
    <property name="targetClass" value="de.hybris.platform.ruleengineservices.rao.CategoryRAO"/>
    <property name="populators">
      <list>
        <ref bean="categoryDataRaoPopulator"/>
      </list>
    </property>
  </bean>
  <alias name="defaultCategoryDataRaoPopulator" alias="categoryDataRaoPopulator"/>
  <bean id="defaultCategoryDataRaoPopulator"
        class="de.hybris.platform.travelrulesengine.converters.populator.CategoryDataRaoPopulator"/>

  <alias name="defaultFareSearchRequestRaoConverter" alias="fareSearchRequestRaoConverter"/>
  <bean id="defaultFareSearchRequestRaoConverter" parent="abstractPopulatingConverter">
    <property name="targetClass" value="de.hybris.platform.travelrulesengine.rao.FareSearchRequestRAO"/>
    <property name="populators">
      <list>
        <ref bean="legInfoRaoPopulator"/>
        <ref bean="passengerTypeQuantityRaoPopulator"/>
      </list>
    </property>
  </bean>

  <alias name="defaultSearchParamsRaoConverter" alias="searchParamsRaoConverter"/>
  <bean id="defaultSearchParamsRaoConverter" parent="abstractPopulatingConverter">
    <property name="targetClass" value="de.hybris.platform.travelrulesengine.rao.SearchParamsRAO"/>
    <property name="populators">
      <list>
        <ref bean="searchParamsRaoBasicPopulator"/>
        <ref bean="searchParamsUserRaoPopulator"/>
        <ref bean="searchParamsLegInfoRaoPopulator"/>
        <ref bean="searchParamsLocationRaoPopulator"/>
        <ref bean="searchParamsOriginatingLocationsRaoPopulator"/>
        <ref bean="searchParamsPassengerTypeQuantityRaoPopulator"/>
      </list>
    </property>
  </bean>
  
  <alias name="defaultOfferRequestRaoConverter" alias="offerRequestRaoConverter"/>
  <bean id="defaultOfferRequestRaoConverter" parent="abstractPopulatingConverter">
    <property name="targetClass" value="de.hybris.platform.travelrulesengine.rao.OfferRequestRAO"/>
    <property name="populators">
      <list>
        <ref bean="offerRequestRaoChannelPopulator"/>
        <ref bean="offerRequestRaoUserRaoPopulator"/>
        <ref bean="offerRequestRaoLegInfoRaoPopulator"/>
        <ref bean="offerRequestRaoLocationRaoPopulator"/>
        <ref bean="offerRequestOriginatingLocationsRaoPopulator"/>
        <ref bean="offerRequestRaoPassengerTypeQuantityPopulator"/>
      </list>
    </property>
  </bean>

  <alias name="defaultTravelCartRaoConverter" alias="cartRaoConverter" />
  <bean id="defaultTravelCartRaoConverter" parent="abstractPopulatingConverter">
    <property name="targetClass" value="de.hybris.platform.ruleengineservices.rao.CartRAO" />
    <property name="populators">
      <list>
        <ref bean="cartRaoPopulator" />
        <ref bean="searchDateCartRaoPopulator" />
        <ref bean="originatingLocationsCartRaoPopulator" />
        <ref bean="passengerTypeQuantityCartRaoPopulator" />
        <ref bean="channelCartRaoPopulator" />
        <ref bean="legInfoCartRaoPopulator"/>
        <ref bean="locationsCartRaoPopulator"/>
      </list>
    </property>
  </bean>
  
  <alias name="defaultLocationsCartRaoPopulator" alias="locationsCartRaoPopulator"/>
  <bean id="defaultLocationsCartRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.LocationsCartRaoPopulator" />
  
  <alias name="defaultOfferRequestRaoChannelPopulator" alias="offerRequestRaoChannelPopulator"/>
  <bean id="defaultOfferRequestRaoChannelPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.OfferRequestRaoChannelPopulator">
    <property name="sessionService" ref="sessionService"/>
  </bean>

  <alias name="defaultOfferRequestRaoUserRaoPopulator" alias="offerRequestRaoUserRaoPopulator"/>
  <bean id="defaultOfferRequestRaoUserRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.OfferRequestRaoUserRaoPopulator">
    <property name="userConverter" ref="userRaoConverter"/>
    <property name="sessionService" ref="sessionService"/>
  </bean>
  
  <alias name="defaultOfferRequestRaoLegInfoRaoPopulator" alias="offerRequestRaoLegInfoRaoPopulator"/>
  <bean id="defaultOfferRequestRaoLegInfoRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.OfferRequestRaoLegInfoRaoPopulator"/>
  
  <alias name="defaultOfferRequestRaoLocationRaoPopulator" alias="offerRequestRaoLocationRaoPopulator"/>
  <bean id="defaultOfferRequestRaoLocationRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.OfferRequestRaoLocationRaoPopulator">
    <property name="ruleTransportFacilityDao" ref="ruleTransportFacilityDao"/>
  </bean>
  
  <alias name="defaultOfferRequestRaoPassengerTypeQuantityPopulator" alias="offerRequestRaoPassengerTypeQuantityPopulator"/>
  <bean id="defaultOfferRequestRaoPassengerTypeQuantityPopulator"
        class="de.hybris.platform.travelrulesengine.converters.populator.OfferRequestRaoPassengerTypeQuantityPopulator">
    <property name="passengerTypeRaoConverter" ref="passengerTypeRaoConverter"/>
  </bean>
  
  <alias name="defaultOfferRequestOriginatingLocationsRaoPopulator" alias="offerRequestOriginatingLocationsRaoPopulator"/>
  <bean id="defaultOfferRequestOriginatingLocationsRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.OfferRequestOriginatingLocationsRaoPopulator"/>
  
  
  <alias name="defaultChannelCartRaoPopulator" alias="channelCartRaoPopulator"/>
  <bean id="defaultChannelCartRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.ChannelCartRaoPopulator">
    <property name="sessionService" ref="sessionService"/>
  </bean>

  <alias name="defaultLegInfoCartRaoPopulator" alias="legInfoCartRaoPopulator"/>
  <bean id="defaultLegInfoCartRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.LegInfoCartRaoPopulator" />

  <alias name="defaultSearchDateCartRaoPopulator" alias="searchDateCartRaoPopulator"/>
  <bean id="defaultSearchDateCartRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.SearchDateCartRaoPopulator">
    <property name="timeService" ref="timeService" />
  </bean>

  <alias name="defaultOriginatingLocationsCartRaoPopulator" alias="originatingLocationsCartRaoPopulator"/>
  <bean id="defaultOriginatingLocationsCartRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.OriginatingLocationsCartRaoPopulator">
    <property name="ruleTravelLocationDao" ref="ruleTravelLocationDao"/>
    <property name="ruleTransportFacilityDao" ref="ruleTransportFacilityDao"/>
  </bean>

  <alias name="defaultSearchParamsUserRaoPopulator" alias="searchParamsUserRaoPopulator"/>
  <bean id="defaultSearchParamsUserRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.SearchParamsUserRaoPopulator">
    <property name="userConverter" ref="userRaoConverter"/>
    <property name="sessionService" ref="sessionService"/>
  </bean>

  <alias name="defaultSearchParamsRaoBasicPopulator" alias="searchParamsRaoBasicPopulator"/>
  <bean id="defaultSearchParamsRaoBasicPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.SearchParamsRaoBasicPopulator">
    <property name="timeService" ref="timeService" />
  </bean>

  <alias name="defaultSearchParamsOriginatingLocationsRaoPopulator" alias="searchParamsOriginatingLocationsRaoPopulator"/>
  <bean id="defaultSearchParamsOriginatingLocationsRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.SearchParamsOriginatingLocationsRaoPopulator"/>

  <alias name="defaultSearchParamsLegInfoRaoPopulator" alias="searchParamsLegInfoRaoPopulator"/>
  <bean id="defaultSearchParamsLegInfoRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.SearchParamsLegInfoRaoPopulator"/>

  <alias name="defaultSearchParamsLocationRaoPopulator" alias="searchParamsLocationRaoPopulator"/>
  <bean id="defaultSearchParamsLocationRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.SearchParamsLocationRaoPopulator">
    <property name="ruleTravelLocationDao" ref="ruleTravelLocationDao"/>
    <property name="ruleTransportFacilityDao" ref="ruleTransportFacilityDao"/>
  </bean>

  <alias name="defaultLegInfoRaoPopulator" alias="legInfoRaoPopulator"/>
  <bean id="defaultLegInfoRaoPopulator" class="de.hybris.platform.travelrulesengine.converters.populator.LegInfoRaoPopulator">
    <property name="timeService" ref="timeService"/>
  </bean>

  <alias name="defaultPassengerTypeQuantityRaoPopulator" alias="passengerTypeQuantityRaoPopulator"/>
  <bean id="defaultPassengerTypeQuantityRaoPopulator"
        class="de.hybris.platform.travelrulesengine.converters.populator.PassengerTypeQuantityRaoPopulator">
    <property name="passengerTypeRaoConverter" ref="passengerTypeRaoConverter"/>
  </bean>

  <alias name="defaultPassengerTypeQuantityCartRaoPopulator" alias="passengerTypeQuantityCartRaoPopulator"/>
  <bean id="defaultPassengerTypeQuantityCartRaoPopulator"
        class="de.hybris.platform.travelrulesengine.converters.populator.PassengerTypeQuantityCartRaoPopulator">
    <property name="passengerTypeRaoConverter" ref="passengerTypeRaoConverter"/>
    <property name="passengerTypeConverter" ref="passengerTypeConverter"/>
  </bean>

  <alias name="defaultSearchParamsPassengerTypeQuantityRaoPopulator" alias="searchParamsPassengerTypeQuantityRaoPopulator"/>
  <bean id="defaultSearchParamsPassengerTypeQuantityRaoPopulator"
        class="de.hybris.platform.travelrulesengine.converters.populator.SearchParamsPassengerTypeQuantityRaoPopulator">
    <property name="passengerTypeRaoConverter" ref="passengerTypeRaoConverter"/>
  </bean>

  <alias name="defaultPassengerTypeRaoConverter" alias="passengerTypeRaoConverter"/>
  <bean id="defaultPassengerTypeRaoConverter" parent="abstractPopulatingConverter">
    <property name="targetClass" value="de.hybris.platform.travelrulesengine.rao.PassengerTypeRAO"/>
    <property name="populators">
      <list>
        <ref bean="passengerTypeRaoPopulator"/>
      </list>
    </property>
  </bean>

  <alias name="defaultPassengerTypeRaoPopulator" alias="passengerTypeRaoPopulator"/>
  <bean id="defaultPassengerTypeRaoPopulator"
        class="de.hybris.platform.travelrulesengine.converters.populator.PassengerTypeRaoPopulator"/>

  <alias name="defaultTravelProductRaoPopulator" alias="productRaoPopulator"/>
  <bean id="defaultTravelProductRaoPopulator"
        class="de.hybris.platform.travelrulesengine.converters.populator.TravelProductRaoPopulator"
        parent="defaultProductRaoPopulator"/>

  <alias name="defaultTravelOrderEntryRaoPopulator" alias="orderEntryRaoPopulator"/>
  <bean id="defaultTravelOrderEntryRaoPopulator"
        class="de.hybris.platform.travelrulesengine.converters.populator.TravelOrderEntryRaoPopulator"
        parent="defaultOrderEntryRaoPopulator"/>

  <!--Action Strategies -->

  <util:map id="travelActionStrategiesMapping">
    <entry key="defaultAddAdminFeeRAOAction" value-ref="adminFeeActionStrategy"/>
    <entry key="defaultRetainAdminFeeRAOAction" value-ref="retainFeeActionStrategy"/>
    <entry key="defaultFareFilteringRAOAction" value-ref="discardFareActionStrategy"/>
    <entry key="defaultBundleFilteringRAOAction" value-ref="discardBundleActionStrategy"/>
    <entry key="defaultShowBundleTemplateRAOAction" value-ref="showBundleTemplateActionStrategy"/>
    <entry key="defaultTransportOfferingFilteringRAOAction" value-ref="discardTransportOfferingActionStrategy"/>
    <entry key="defaultShowProductRAOAction" value-ref="showProductsActionStrategy" />
    <entry key="defaultShowProductCategoriesRAOAction" value-ref="showProductCategoriesActionStrategy" />
  </util:map>


  <bean id="abstractTravelRuleActionStrategy" abstract="true"
        class="de.hybris.platform.travelrulesengine.action.impl.AbstractTravelRuleActionStrategy"/>

  <alias name="defaultAdminFeeActionStrategy" alias="adminFeeActionStrategy"/>
  <bean id="defaultAdminFeeActionStrategy" parent="abstractTravelRuleActionStrategy"
        class="de.hybris.platform.travelrulesengine.action.impl.DefaultAdminFeeActionStrategy">
    <property name="productService" ref="productService"/>
    <property name="cartService" ref="cartService"/>
    <property name="commerceCartService" ref="commerceCartService"/>
    <property name="commercePriceService" ref="commercePriceService"/>
    <property name="modelService" ref="modelService"/>
  </bean>

  <alias name="defaultRetainFeeActionStrategy" alias="retainFeeActionStrategy"/>
  <bean id="defaultRetainFeeActionStrategy" parent="abstractTravelRuleActionStrategy"
        class="de.hybris.platform.travelrulesengine.action.impl.DefaultRetainFeeActionStrategy">
  </bean>

  <alias name="defaultDiscardFareActionStrategy" alias="discardFareActionStrategy"/>
  <bean id="defaultDiscardFareActionStrategy" parent="abstractTravelRuleActionStrategy"
        class="de.hybris.platform.travelrulesengine.action.impl.DefaultDiscardFareActionStrategy"/>

  <alias name="defaultDiscardBundleActionStrategy" alias="discardBundleActionStrategy"/>
  <bean id="defaultDiscardBundleActionStrategy" parent="abstractTravelRuleActionStrategy"
        class="de.hybris.platform.travelrulesengine.action.impl.DefaultDiscardBundleActionStrategy"/>

  <alias name="defaultShowBundleTemplateActionStrategy" alias="showBundleTemplateActionStrategy"/>
  <bean id="defaultShowBundleTemplateActionStrategy" parent="abstractTravelRuleActionStrategy"
        class="de.hybris.platform.travelrulesengine.action.impl.DefaultShowBundleTemplateActionStrategy"/>
        
  <alias name="defaultShowProductsActionStrategy" alias="showProductsActionStrategy"/>
  <bean id="defaultShowProductsActionStrategy" parent="abstractTravelRuleActionStrategy"
        class="de.hybris.platform.travelrulesengine.action.impl.DefaultShowProductsActionStrategy"/>
        
  <alias name="defaultShowProductCategoriesActionStrategy" alias="showProductCategoriesActionStrategy"/>
  <bean id="defaultShowProductCategoriesActionStrategy" parent="abstractTravelRuleActionStrategy"
        class="de.hybris.platform.travelrulesengine.action.impl.DefaultShowProductCategoriesActionStrategy"/>      

  <alias name="defaultDiscardTransportOfferingActionStrategy" alias="discardTransportOfferingActionStrategy"/>
